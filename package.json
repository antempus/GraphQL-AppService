{
  "name": "azure-app-service",
  "version": "0.1.0",
  "description": "graphql app service",
  "repository": {
    "type": "git",
    "url": "https://github.com/antempus/azure-app-service"
  },
  "author": "Robert N. Steele",
  "scripts": {
