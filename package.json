    "graphql": "^15.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/eslint": "^6.1.1",
    "@types/node": "^12.7.8",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint": "^6.4.0",
    "nodemon": "^1.19.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
